            <tr>
                <th>Classification</th>
                <td>{{ participate_to_competition.classification }}</td>
            </tr>
            <tr>
                <th>Prize</th>
                <td>{{ participate_to_competition.prize }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_participate_to_competition_index') }}">back to list</a>

    <a href="{{ path('app_participate_to_competition_edit', {'id': participate_to_competition.id}) }}">edit</a>

    {{ include('participate_to_competition/_delete_form.html.twig') }}
