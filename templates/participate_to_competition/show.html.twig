{% block title %}ParticipateToCompetition{% endblock %}

{% block body %}
    <h1>ParticipateToCompetition</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ participate_to_competition.id }}</td>
            </tr>
            <tr>
                <th>IsPresent</th>
                <td>{{ participate_to_competition.isPresent ? 'Yes' : 'No' }}</td>
            </tr>
